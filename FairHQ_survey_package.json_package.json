    "web-vitals": "^1.0.1",
    "yup": "^0.32.9"
  },
  "scripts": {
    "build": "REACT_APP_VERSION=$npm_package_version react-scripts build",
    "build:less": "yarn lessc --js src/styles.less src/styles.css",
    "prebuild": "node meta && yarn build:less",
    "lint": "yarn lint:style && yarn lint:js",
    "lint:js": "eslint . --ext .ts,.tsx",
    "lint:debug": "eslint . --ext .ts,.tsx --fix --debug",
    "lint:style": "stylelint './src/**/*.{ts,tsx}'",
    "start": "REACT_APP_VERSION=$npm_package_version react-scripts start",
    "start:local": "REACT_APP_ENV=local REACT_APP_VERSION=$npm_package_version react-scripts start",
    "prestart": "node meta && yarn build:less",
    "prestart:local": "node meta && yarn build:less",
    "test": "react-scripts test",
    "test:ci": "CI=true TZ=UTC yarn test --coverage --ci --maxWorkers=2 -u"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullstory/browser": "^1.5.1",
    "@types/cypress": "^1.1.3",
    "@types/i18next": "^13.0.0",
    "@types/lodash": "^4.14.170",
    "@types/node-fetch": "^2.6.1",
    "@types/react-i18next": "^8.1.0",
    "@types/redux-logger": "^3.0.9",
    "@types/redux-mock-store": "^1.0.2",
    "@types/styled-components": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "cypress": "^9.5.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-cucumber": "^1.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "less": "^3.12.2",
    "less-loader": "^7.1.0",
    "prettier": "^2.6.1",
    "stylelint": "^14.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "resolutions": {
    "**/trim": "^1.0.0"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!(@fairhq/common)/)",
      "\\.pnp\\.[^\\/]+$"
    ],
    "moduleNameMapper": {
      "^.+\\.(css)$": "<rootDir>/src/test/jest-css-mock.ts"
    }
  }
}
