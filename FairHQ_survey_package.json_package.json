    "date-fns": "^2.28.0",
    "dayjs": "^1.10.5",
    "i18next": "^20.3.1",
    "i18next-browser-languagedetector": "^6.1.1",
    "i18next-http-backend": "^1.3.2",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.10.0",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "5.0.0",
    "react-use": "^17.2.4",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "rsuite": "^4.10.2",
    "schema-typed": "^2.0.0",
    "styled-components": "^5.3.0",
    "theme-ui": "^0.14.1",
